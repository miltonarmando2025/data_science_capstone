    "import pandas as pd\n",
    "\n",
    "# Load raw data\n",
    "df = pd.read_csv('../data/raw_data.csv')\n",
    "\n",
    "# Clean data: Handle missing ages\n",
    "df['age'] = df['age'].fillna(df['age'].median())\n",
    "\n",
    "# Save cleaned data\n",
    "df.to_csv('../data/cleaned_data.csv', index=False)"
   ]
  }
